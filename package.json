{
    "name": "yonius",
    "version": "0.2.6",
    "description": "The Yonius library",
    "keywords": [
        "api",
        "foundation",
        "js",
        "library",
        "yonius"
    ],
    "homepage": "https://github.com/hivesolutions/yonius#readme",
    "bugs": {
        "url": "https://github.com/hivesolutions/yonius/issues"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/hivesolutions/yonius.git"
    },
    "license": "Apache-2.0",
    "author": "Hive Solutions",
    "files": [
        "LICENSE",
        "dist/**/*",
        "res/**/*",
        "js/**/*.js",
        "test/**/*.js"
    ],
    "main": "dist/yonius.cjs.js",
    "unpkg": "dist/yonius.umd.js",
    "module": "dist/yonius.esm.js",
    "browser": "dist/yonius.umd.js",
    "scripts": {
        "build": "rollup -c",
        "example": "node examples/simple.js",
        "lint": "eslint \"./{js,test}/**/*.{js,json}\"",
        "lint-fix": "eslint \"./{js,test}/**/*.{js,json}\" --fix",
        "prettier": "prettier \"./*.{js,json}\" \"./{js,test}/**/*.{js,json}\" --write",
        "pretty": "npm run prettier && npm run lint-fix",
        "test": "mocha --recursive",
        "upgrade": "npx sort-package-json && ncu -u",
        "watch": "rollup -c -w"
    },
    "dependencies": {
        "node-fetch": "^2.6.0"
    },
    "devDependencies": {
        "@babel/core": "^7.6.4",
        "@babel/preset-env": "^7.6.3",
        "eslint": "^6.5.1",
        "eslint-config-hive": "^0.1.22",
        "mocha": "^6.2.2",
        "mocha-cli": "^1.0.1",
        "npm-check-updates": "^3.1.24",
        "prettier": "^1.18.2",
        "prettier-config-hive": "^0.1.1",
        "rollup": "^1.25.0",
        "rollup-plugin-babel": "^4.3.3",
        "rollup-plugin-commonjs": "^10.1.0",
        "rollup-plugin-node-builtins": "^2.1.2",
        "rollup-plugin-node-globals": "^1.4.0",
        "rollup-plugin-node-resolve": "^5.2.0",
        "sort-package-json": "^1.22.1"
    }
}
